{
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/.well-known/webfinger",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/.well-known/host-meta",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/.well-known/nodeinfo",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/.well-known/webfinger",
      "destination": "https://social.laomai.org/.well-known/webfinger",
      "permanent": true
    },
    {
      "source": "/.well-known/host-meta",
      "destination": "https://social.laomai.org/.well-known/host-meta",
      "permanent": true
    },
    {
      "source": "/.well-known/nodeinfo",
      "destination": "https://social.laomai.org/.well-known/nodeinfo",
      "permanent": true
    }
  ]
}
